n\" : \"Add new user\"}\r\n        </h1>\r\n        <NavigationButton\r\n          onClick={() => {\r\n            navigate(\"/\");\r\n          }}\r\n        >\r\n          Go Back\r\n        </NavigationButton>\r\n      </Plate>\r\n      <Form onSubmit={_handleFormSubmission}>\r\n        <label>First and Last Name:</label>\r\n        <input\r\n          type=\"text\"\r\n          required\r\n          {...nameBindings}\r\n          pattern=\"[A-Za-z]{3,15} [A-Za-z]{3,15}\"\r\n          placeholder=\"Enter your full name\"\r\n        />\r\n        <label>Email</label>\r\n        <input\r\n          type=\"email\"\r\n          required\r\n          {...emailBindings}\r\n          placeholder=\"E.g. example@website.com\"\r\n        />\r\n        <label>Phone</label>\r\n        <input\r\n          type=\"tel\"\r\n          required\r\n          {...phoneBindings}\r\n          placeholder=\"03081234567\"\r\n        />\r\n        <label>Gender:</label>\r\n        <div className=\"genderPicker\">\r\n          <label htmlFor=\"genderMale\">Male</label>\r\n          <input\r\n            id=\"genderMale\"\r\n            type=\"radio\"\r\n            name=\"gender\"\r\n            checked={\"M\" === gender}\r\n            value=\"M\"\r\n            required\r\n            {...genderBindings}\r\n          />\r\n          <label htmlFor=\"genderFemale\">Female</label>\r\n          <input\r\n            id=\"genderFemale\"\r\n            type=\"radio\"\r\n            name=\"gender\"\r\n            checked={\"F\" === gender}\r\n            value=\"F\"\r\n            required\r\n            {...genderBindings}\r\n          />\r\n        </div>\r\n        <label>Country</label>\r\n        <select required {...countryBindings}>\r\n          <option disabled value=\"\">\r\n            Please select a country\r\n          </option>\r\n          {countries.map((_country, _index) => (\r\n            <option value={_country} key={_index}>\r\n              {_country}\r\n            </op